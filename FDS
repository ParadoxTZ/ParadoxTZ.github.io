    if (!window.MathJax) {
      window.MathJax = {
        tex: {
          inlineMath: [['$', '$'], ['\\(', '\\)']],
          displayMath: [['$$', '$$'], ['\\[', '\\]']],
          tags: 'none',
        },
        chtml: {
          scale: 1.1
        },
        startup: {
          typeset: false
        },
        options: {
          enableMenu: true,
          // 扩展渲染动作，处理 <span class="math"> 元素
          renderActions: {
            findScript: [10, function (doc) {
              // 处理 <script type="math/tex"> 标签
              for (const node of document.querySelectorAll('script[type^="math/tex"]')) {
                const display = !!node.type.match(/; *mode=display/)
                const math = new doc.options.MathItem(node.textContent, doc.inputJax[0], display)
                const text = document.createTextNode('')
                node.parentNode.replaceChild(text, node)
                math.start = {node: text, delim: '', n: 0}
                math.end = {node: text, delim: '', n: 0}
                doc.math.push(math)
              }
              // 处理 <span class="math"> Pandoc输出的公式
              for (const node of document.querySelectorAll('#article-container .math')) {
                if (node.classList.contains('MathJax-rendered')) continue
                const display = node.classList.contains('display')
                const tex = node.textContent.trim()
                if (tex) {
                  const math = new doc.options.MathItem(tex, doc.inputJax[0], display)
                  const text = document.createTextNode('')
                  node.parentNode.replaceChild(text, node)
                  math.start = {node: text, delim: '', n: 0}
                  math.end = {node: text, delim: '', n: 0}
                  doc.math.push(math)
                  node.classList.add('MathJax-rendered')
                }
              }
            }, '']
          }
        }
      }

      const script = document.createElement('script')
      script.src = 'https://cdn.jsdelivr.net/npm/mathjax@3.2.2/es5/tex-mml-chtml.min.js'
      script.id = 'MathJax-script'
      script.async = true
      document.head.appendChild(script)
    } else {
      MathJax.startup.document.state(0)
      MathJax.texReset()
      MathJax.typesetPromise()
    }
  }

  btf.addGlobalFn('encrypt', loadMathjax, 'mathjax')
